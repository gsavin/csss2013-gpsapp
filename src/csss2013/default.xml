<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties version="1.0">
	<comment>This is the default properties file.</comment>
	<entry key="app.title">CSSS2013 : GPS App</entry>
	<entry key="app.wizard">true</entry>
	
	<entry key="trace.time.round">true</entry>

	<entry key="process.reload.min_distance">40</entry>
	<entry key="process.reload.checklinks">true</entry>

	<entry key="views.dynamic.tick">10</entry>
	<entry key="views.dynamic.stylesheet">node {size:15px;}</entry>
	<entry key="views.static.stylesheet">node {size:10px;} edge {arrow-size:2px,2px;}</entry>
</properties>